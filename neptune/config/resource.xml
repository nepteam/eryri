<?xml version="1.0" encoding="utf-8"?>
<application>
    <routes>
        <resource
            pattern="/vf/**"
            location="static-vendors/foundation-4.2"
            regexp="false"
            cache="true"
        />
        <resource
            pattern="/vb/**"
            location="static-vendors/bootstrap"
            regexp="false"
            cache="true"
        />
        <resource
            pattern="/image/**"
            location="resources/image"
            regexp="false"
            cache="false"
        />
        <resource
            pattern="/js/**"
            location="resources/js"
            regexp="false"
            cache="false"
        />
        <resource
            pattern="/css/**"
            location="resources/css"
            regexp="false"
            cache="false"
        />
    </routes>
</application>